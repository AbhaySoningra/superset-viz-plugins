{
  "name": "@superset-maf-ui/plugin-chart-bar",
  "version": "0.0.0",
  "description": "Superset Chart - Bar",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "files": [
    "esm",
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "tdd": "NODE_ENV=test jest --watch",
    "build": "NODE_ENV=development webpack --mode=development --colors",
    "build:watch": "NODE_ENV=development webpack --mode=development --colors --watch",
    "build:prod": "BABEL_ENV=production babel src -d dist --verbose --extensions \".ts,.tsx,.js,.jsx \" --ignore \"src/**/*.test.tsx\",\"src/*.test.js\",\"src/**/*.mock.js\",\"src/**/*.stories.tsx\",\"src/**/__stories__/*\",\"src/**/__mocks__/*\",\"src/**/__tests__/*\"",
    "build:esm": "ESM=true BABEL_ENV=production babel src -d esm --verbose --extensions \".ts,.tsx,.js,.jsx \" --ignore \"src/**/*.test.tsx\",\"src/*.test.js\",\"src/**/*.mock.js\",\"src/**/*.stories.tsx\",\"src/**/__stories__/*\",\"src/**/__mocks__/*\",\"src/**/__tests__/*\"",
    "serve": "http-server dist -c-1 --cors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/nielsen-media/maf/superset/superset-maf-ui.git"
  },
  "keywords": [
    "superset"
  ],
  "author": "MAF",
  "bugs": {
    "url": "https://gitlab.com/nielsen-media/maf/superset/superset-maf-ui/issues"
  },
  "homepage": "https://gitlab.com/nielsen-media/maf/superset/superset-maf-ui#readme",
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/recharts": "^1.8.14",
    "jest": "^26.0.1"
  },
  "dependencies": {
    "@superset-ui/chart": "^0.14.1",
    "@superset-ui/chart-controls": "^0.14.0",
    "@superset-ui/color": "^0.14.23",
    "@superset-ui/core": "^0.14.23",
    "@superset-ui/dimension": "^0.14.9",
    "@superset-ui/number-format": "^0.14.23",
    "@superset-ui/query": "^0.14.1",
    "@superset-ui/style": "^0.14.0",
    "@superset-ui/translation": "^0.14.0",
    "@superset-ui/validator": "^0.14.1",
    "recharts": "^1.8.5"
  },
  "peerDependencies": {
    "react": "^16.13.1"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
